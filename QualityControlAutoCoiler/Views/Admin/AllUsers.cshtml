@model Entities.ViewModels.AppUser
@{
    ViewData["Title"] = "Register User";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form method="post" id="EditUser" action="/Admin/EditUser">   
</form>
<form method="post" id="ViewUser" action="/Admin/ViewUser">    
</form>
<!-- BEGIN : Main Content-->
<div class="main-content">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-sm-12">
                <div class="content-header">ALL USERS</div>
                <p class="content-sub-header">List of all the users of portal</p>
            </div>
        </div>
        <div class="card">
            <div class="card-content">
                <div class="card-body">
                    <table class="table table-striped table-bordered" Id="AllUsers">
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
    <partial name="_ScriptDataTable" />
    <partial name="_ScriptSweetAlert" />
    <script src="~/Scripts/CommonJS.js"></script>
    <script src="~/Scripts/Admin.js"></script>
}